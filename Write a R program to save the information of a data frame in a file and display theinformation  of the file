# Load the readr library
library(readr)
# Create the exam data frame
exam_data = data.frame( name = c("Anastasia", "Dima", "Katherine", "James", "Emily"),
score = c(12.5, 9, 16.5, 12, 9), 
attempts = c(1, 3, 2, 3, 2), 
qualify = c("yes", "no", "yes", "no", "no") )
# Write the exam data frame to a CSV file
write_csv(exam_data, "exam_data.csv")
# Read the CSV file and display the information
print(read_csv("exam_data.csv"))
