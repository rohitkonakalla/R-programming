data(mpg)
disp <- mpg$disp
disp_range <- range(disp)
cat("The range of the displacement column is:", disp_range[1], "to", disp_range[2], "\n")
disp_quartiles <- quantile(disp, probs = c(0.25, 0.5, 0.75))
cat("The quartiles of the displacement column are:", disp_quartiles, "\n")
disp_iqr <- IQR(disp)
cat("The interquartile range (IQR) of the displacement column is:", disp_iqr, "\n")
